<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seatplanner.MenuController">
   <children>
      <Label fx:id="welcomeLabel" alignment="TOP_LEFT" layoutX="53.0" layoutY="74.0" prefHeight="119.0" prefWidth="569.0" text="Welcome">
         <font>
            <Font name="Segoe Fluent Icons" size="41.0" />
         </font>
      </Label>
      <Button fx:id="studentButton" layoutX="530.0" layoutY="336.0" mnemonicParsing="false" onAction="#switchToStudents" prefHeight="60.0" prefWidth="184.0" text="Add Students" />
      <Button layoutX="101.0" layoutY="337.0" mnemonicParsing="false" onAction="#switchToPlanner" prefHeight="67.0" prefWidth="172.0" text="Planner" />
      <Label layoutX="53.0" layoutY="125.0" text="You must add at least 1 student before advancing to the planner!" textFill="#012826" />
   </children>
</AnchorPane>
